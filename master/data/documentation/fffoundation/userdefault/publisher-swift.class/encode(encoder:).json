    },
    {
      "kind" : "parameters",
      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "An encoder that implements the ",
                  "type" : "text"
                },
                {
                  "code" : "TopLevelEncoder",
                  "type" : "codeVoice"
                },
                {
                  "text" : " protocol.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
